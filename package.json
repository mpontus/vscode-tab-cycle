{
    "name": "tab-cycle",
    "displayName": "Tab Cycle",
    "description": "Delightful tab navigation technique",
    "version": "0.0.1",
    "publisher": "mpontus",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "keybindings": {
            "command": "tabCycle.buryActiveEditor",
            "key": "ctrl+pagedown",
            "mac": "cmd+pagedown"
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
